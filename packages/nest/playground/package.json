{
  "name": "@usex/raft-nestjs-playground",
  "version": "1.0.0",
  "description": "Interactive playground for @usex/raft-nestjs",
  "private": true,
  "scripts": {
    "start": "nest start --watch",
    "start:cluster": "concurrently \"npm:start:node1\" \"npm:start:node2\" \"npm:start:node3\"",
    "start:node1": "cross-env NODE_ID=node-1 HTTP_PORT=3001 nest start --watch",
    "start:node2": "cross-env NODE_ID=node-2 HTTP_PORT=3002 nest start --watch", 
    "start:node3": "cross-env NODE_ID=node-3 HTTP_PORT=3003 nest start --watch",
    "start:monitoring": "concurrently \"npm:start:cluster\" \"npm:start:dashboard\"",
    "start:dashboard": "nest start dashboard --watch",
    "start:cli": "nest start cli",
    "test:load": "artillery run tests/load-test.yml",
    "test:chaos": "node scripts/chaos-test.js",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.0",
    "@nestjs/core": "^11.0.0",
    "@nestjs/platform-express": "^11.0.0",
    "@nestjs/platform-socket.io": "^11.0.0",
    "@nestjs/websockets": "^11.0.0",
    "@nestjs/swagger": "^7.0.0",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/event-emitter": "^2.0.0",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/bull": "^10.0.0",
    "@usex/raft-nestjs": "workspace:*",
    "@usex/raft": "workspace:*",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.0",
    "bull": "^4.11.0",
    "ioredis": "^5.3.0",
    "socket.io": "^4.6.0",
    "prom-client": "^15.0.0",
    "class-validator": "^0.14.0",
    "class-transformer": "^0.5.0",
    "helmet": "^7.0.0",
    "compression": "^1.7.0",
    "chalk": "^4.1.0",
    "inquirer": "^8.2.0",
    "ora": "^5.4.0",
    "cli-table3": "^0.6.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.0",
    "@types/node": "^20.0.0",
    "@types/express": "^5.0.0",
    "@types/socket.io": "^3.0.0",
    "@types/inquirer": "^8.2.0",
    "concurrently": "^8.0.0",
    "cross-env": "^7.0.0",
    "nodemon": "^3.0.0",
    "artillery": "^2.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  }
}